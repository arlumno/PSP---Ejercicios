package tesTing;

import java.io.File;

public class argumentosMainMaster {

	public static void main(String[] args) {
		
		ProcessBuilder pb= new ProcessBuilder("java","tesTing.argumentosMain");
		pb.directory(new File(".\\bin"));	
		try {
			
		Process p = pb.start();
		}catch (Exception e) {
			System.out.println(e);
		}
		
		

	}

}
